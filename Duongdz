    [1] = 1,2,
    [2] = game:GetService("Players").LocalPlayer
}

game:GetService("Players").LocalPlayer.Character.Venom.CaiNayDungDeDanh.RemoteEvent:FireServer(unpack(args))
end)
end
end
end)

